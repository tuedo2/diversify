{
    "name": "Diversify-Simple",
    "description": "Diversifying your Twitter feed",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "webNavigation"
    ],
    "host_permissions": [
        "https://twitter.com/*"
    ]
}
